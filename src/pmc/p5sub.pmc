/* Various Perl 5 headers that we need. */
#undef __attribute__
#undef __attribute__noreturn__
#undef __attribute__deprecated__
#define __attribute__(x) /* */
#include <EXTERN.h>
#include <perl.h>

pmclass P5Sub group blizkost_group dynpmc {

/*

=item C<void init()>

Set up P5Sub PMC.


=cut

*/

    VTABLE void init() {
    }


/*

=item C<INTVAL get_bool()>

Returns the boolean value of the P5Sub

=cut

*/

    VTABLE INTVAL get_bool() {
        return SELF.get_integer() ? 1 : 0;
    }


/*

=item C<INTVAL get_integer()>

Returns the integer value of the Integer.

=cut

*/

    VTABLE INTVAL get_integer() {
    }


}
